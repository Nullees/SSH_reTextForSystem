<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping>  
	<!-- 每个class对应一个持久化对象 -->
	<class name="com.edu.po.Course">
		<!-- id元素用来定义主键标示，并制定主键生成策略 -->
		<id name="c_no">
			<generator class="assigned"></generator>
		</id>
		
		<!-- 定义其他属性 -->
		<property name="c_name"></property>
		<property name="c_credit"></property>
		<property name="c_time"></property>
		<property name="c_point"></property>
		<property name="c_type"></property>
		<property name="t_no"></property>
	</class>
</hibernate-mapping>  
